{
  "name": "mellins-shopify-vouchers",
  "main": "dist/index.mjs",
  "author": "A-J Roos <asjas@hey.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "scripts": {
    "start": "node dist/index.mjs",
    "cli": "node dist/cli.mjs",
    "build": "rimraf dist && babel --extensions '.ts' src/ --out-dir dist/ --out-file-extension .mjs",
    "dev": "tsnd --transpile-only --respawn src/index.ts",
    "test": "tap",
    "test:w": "tap -w",
    "test:inspect": "tap -O -T --node-arg=--inspect-brk",
    "coverage": "npm test -- --coverage-report=lcov",
    "check-types": "tsc",
    "lint": "eslint --ext .ts .",
    "lint-fix": "npm run lint -- --fix",
    "prettier": "prettier --ignore-path .eslintignore \"**/*.+(ts)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint build"
  },
  "dependencies": {
    "dotenv": "10.0.0",
    "fastify": "3.20.1",
    "fastify-favicon": "3.1.0",
    "fastify-shopify-graphql-proxy": "0.12.1",
    "graphql": "15.5.1",
    "graphql-request": "3.5.0",
    "pino-pretty": "5.1.2",
    "waait": "1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.15.0",
    "@babel/plugin-syntax-top-level-await": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@types/node": "16.0.0",
    "@types/tap": "15.0.5",
    "babel-plugin-module-extension-resolver": "1.0.0-rc.2",
    "eslint-config-asjas": "0.0.25",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "tap": "15.0.9",
    "ts-node": "10.2.0",
    "ts-node-dev": "1.1.6",
    "typescript": "4.3.5"
  }
}
